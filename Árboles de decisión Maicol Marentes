   "metadata": {},
   "outputs": [],
   "source": [
    "# Crear el modelo de Árbol de Decisión con hiperparámetros ajustados\n",
    "tree = DecisionTreeClassifier(\n",
    "    criterion='gini',  # o 'entropy'\n",
    "    max_depth=5,  # Controla la profundidad máxima del árbol\n",
    "    min_samples_split=10,  # Mínimo número de muestras para dividir un nodo\n",
    "    min_samples_leaf=5,  # Mínimo número de muestras en las hojas\n",
    "    max_features='sqrt',  # Selecciona un subconjunto de características\n",
    "    random_state=42  # Asegura reproducibilidad\n",
    ")\n"
   ]
  },
